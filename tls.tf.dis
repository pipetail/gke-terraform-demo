resource "google_compute_managed_ssl_certificate" "jarvis-dev-indykite" {
  provider = google-beta

  name = "jarvis-dev-cert"

  lifecycle {
    create_before_destroy = true
  }

  managed {
    domains = ["jarvis-dev.indykite.com."]
  }

  depends_on = [
    google_project_service.services["compute.googleapis.com"],
  ]
}

